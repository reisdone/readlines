{
  "name": "challenge",
  "version": "1.0.0",
  "description": "Salsify challenge",
  "main": "build/server.js",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./app/server.ts",
    "start:watch": "nodemon",
    "build": "tsc"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "app"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.16.1",
    "express": "^4.16.4",
    "typescript": "^3.4.2"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.16.0",
    "nodemon": "^1.18.11",
    "ts-node": "^8.0.3"
  }
}
